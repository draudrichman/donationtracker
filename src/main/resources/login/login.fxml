<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.login_controller">
    <children>
        <Label alignment="CENTER" layoutX="419.0" layoutY="8.0" prefHeight="103.0" prefWidth="228.0" text="GivingTree" textAlignment="CENTER" textFill="#15a85c">
            <font>
                <Font name="System Bold" size="43.0" />
            </font>
        </Label>
        <PasswordField fx:id="Password" layoutX="426.0" layoutY="348.0" prefHeight="25.0" prefWidth="214.0" />
        <TextField fx:id="UserName" layoutX="426.0" layoutY="289.0" prefHeight="25.0" prefWidth="214.0" />
        <Label layoutX="426.0" layoutY="326.0" text="Password">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Label layoutX="426.0" layoutY="267.0" text="Email">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Button fx:id="LOGIN" layoutX="426.0" layoutY="384.0" mnemonicParsing="false" onAction="#logIn" prefHeight="25.0" prefWidth="214.0" text="Log In" />
        <Button fx:id="Reset_Password" layoutX="426.0" layoutY="419.0" mnemonicParsing="false" onAction="#resetPassword" text="Reset Password" />
        <Button fx:id="SIGNUP" layoutX="540.0" layoutY="419.0" mnemonicParsing="false" onAction="#signUp" prefHeight="25.0" prefWidth="99.0" text="Sign Up" />
