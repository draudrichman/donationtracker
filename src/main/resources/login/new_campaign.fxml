            <Font name="System Bold" size="43.0" />
         </font>
      </Label>
      <TextField fx:id="CampaignTitle" layoutX="290.0" layoutY="180.0" prefHeight="30.0" prefWidth="500.0" />
      <Label layoutX="290.0" layoutY="155.0" text="Camaign title:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="290.0" layoutY="225.0" text="Campaign Category:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="GoalAmount" layoutX="290.0" layoutY="320.0" prefHeight="30.0" prefWidth="500.0" />
      <Label layoutX="290.0" layoutY="295.0" text="Goal Amount:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="290.0" layoutY="365.0" text="Campaign Description:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="290.0" layoutY="555.0" text="Confirm Password :">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="CampaignDescription" layoutX="290.0" layoutY="390.0" prefHeight="150.0" prefWidth="500.0" />
      <PasswordField fx:id="Confirm_Password" layoutX="290.0" layoutY="580.0" prefHeight="30.0" prefWidth="500.0" />
      <Button fx:id="CreateNewCampaign" layoutX="439.0" layoutY="640.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0" text="Create New Campaign" />
      <Button fx:id="GoBackToHome" layoutX="439.0" layoutY="690.0" mnemonicParsing="false" onAction="#goToHome" prefHeight="30.0" prefWidth="200.0" text="Go back to Home" />
      <TextField fx:id="CampaignTitle1" layoutX="290.0" layoutY="250.0" prefHeight="30.0" prefWidth="500.0" />
      <MenuBar layoutY="115.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="28.0" prefWidth="1080.0">
         <menus>
            <Menu mnemonicParsing="false" text="Home">
               <items>
                  <MenuItem fx:id="Home" mnemonicParsing="false" text="Home" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Campaigns">
               <items>
                  <MenuItem fx:id="Explore" mnemonicParsing="false" text="Explore" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem fx:id="YourCampaign" mnemonicParsing="false" text="Your Campaigns" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem fx:id="DonatedCampaign" mnemonicParsing="false" text="Campaigns you donated to" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Profile">
               <items>
                  <MenuItem fx:id="MyProfile" mnemonicParsing="false" onAction="#goToProfile" text="My Profile" />
